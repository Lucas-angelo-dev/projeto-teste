algoritmo "Creuza01"
var
  ano_atual, ano_nasc, idade:real
inicio
      escreva("Em que ano nòs estamos? ")
      leia(ano_atual)
      escreval("Em que ano eu nasci ? ")
      leia(ano_nasc)
      idade <- ano_atual - ano_nasc
      escreva(" minha idade sera ?",idade)
      











fimalgoritmo
      
      


       

     



